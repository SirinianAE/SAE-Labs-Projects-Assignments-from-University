package taxSystem;

public class HeadOfHouseholdTaxpayer extends Taxpayer {
	public double getTax(){
		float income = getIncome();
		if (0>=income && income<30390){
			return 5.35*income / 100;
		}
		else if(30390>=income && income<90000){
			return 1625.87 + 7.05*(income - 30390)/100;
		}
		else if(90000>=income && income<122110){
			return 5828.38 + 7.05*(income - 90000)/100;
		}
		else if(122110>=income && income<203390){
			return 8092.13 + 7.85*(income - 122110)/100;
		}
		else{
			return 14472.61 + 9.85*(income - 203390)/100;
		}
	}	
	
	setFamilyStatus("HeadOfHousehold");
}